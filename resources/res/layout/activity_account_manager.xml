<?xml version="1.0" encoding="utf-8"?>
<b.ContentWrapper xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Space
            android:id="@+id/status_bar_mask"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:insetType="topMargin"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.security.xvpn.z35kb.widget.AccountHeaderBackgroundView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="720:238"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/btnBack"
            android:focusable="true"
            android:src="@drawable/icon_nav_back_white"
            android:scaleType="centerCrop"
            android:contentDescription="Back"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/status_bar_mask"
            style="@style/TopBarIconStyle"/>
        <TextView
            android:textSize="20sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/tv_nav_sign_in"
            android:background="@drawable/bg_focus_rectangle"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:focusable="true"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:text="@string/SignIn"
            android:contentDescription="double tap to sign in page"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/tv_nav_sign_up"
            app:layout_constraintTop_toBottomOf="@+id/status_bar_mask"/>
        <TextView
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/tv_nav_sign_up"
            android:background="@drawable/bg_focus_rectangle"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:focusable="true"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:text="@string/SignUp"
            android:contentDescription="Now, you are in sign up page"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintLeft_toRightOf="@+id/tv_nav_sign_in"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/status_bar_mask"/>
        <FrameLayout
            android:id="@+id/contentPanel"
            android:background="@drawable/bg_account_panel"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnBack"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ivIndicator"
            android:layout_width="22dp"
            android:layout_height="14dp"
            android:layout_marginTop="45dp"
            android:src="@drawable/ic_account_indicator"
            app:layout_constraintLeft_toLeftOf="@+id/tv_nav_sign_in"
            app:layout_constraintRight_toRightOf="@+id/tv_nav_sign_in"
            app:layout_constraintTop_toTopOf="@+id/status_bar_mask"/>
    </com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
</b.ContentWrapper>
