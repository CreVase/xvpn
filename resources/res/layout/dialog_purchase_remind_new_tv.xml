<?xml version="1.0" encoding="utf-8"?>
<com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/img_purchase_remind_top_tv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:statusBarMaskColor="@color/transparent">
    <ImageView
        android:id="@+id/vTopBg"
        android:layout_width="match_parent"
        android:layout_height="280dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/purchase_remind_top_panel"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:insetType="topPadding"
        app:layout_constraintBottom_toBottomOf="@+id/vTopBg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="@dimen/dialog_purchase_remind_title_txt_size"
            android:textColor="@color/white"
            android:layout_gravity="center_horizontal"
            android:id="@+id/purchase_remind_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dialog_purchase_remind_padding_left"
            android:layout_marginTop="@dimen/dialog_purchase_remind_title_margin_top"
            android:layout_marginRight="@dimen/dialog_purchase_remind_padding_right"
            android:text="Congratulations"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="@dimen/dialog_purchase_remind_upgrade_success_txt_size"
            android:textColor="@color/white"
            android:layout_gravity="center_horizontal"
            android:id="@+id/purchase_remind_subtitle_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dialog_purchase_remind_padding_left"
            android:layout_marginTop="@dimen/dialog_purchase_remind_upgrade_success_txt_margin_top"
            android:layout_marginRight="@dimen/dialog_purchase_remind_padding_right"
            android:text="@string/PremiumNoAccountSuccess"
            android:lineSpacingExtra="5dp"/>
        <Space
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <View
            android:id="@+id/vArch"
            android:background="@drawable/img_purchase_top_arch"
            android:layout_width="match_parent"
            android:layout_height="58dp"/>
    </LinearLayout>
    <View
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/purchase_remind_top_panel"/>
    <ImageView
        android:id="@+id/vClose"
        android:background="@drawable/bg_status_bar_icon_tv"
        android:focusable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/ic_promo_guide_close"
        android:layout_marginEnd="15dp"
        app:insetType="topPadding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/ivAvatar"
        android:layout_width="101dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_purchase_succeed_avatar"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="@+id/purchase_remind_top_panel"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_percent="0.16"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
    <TextView
        android:textSize="@dimen/dialog_purchase_remind_upgrade_success_txt_size"
        android:textColor="#465c66"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:id="@+id/purchase_remind_notice_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="35dp"
        android:text="@string/PremiumSignUpTips"
        android:lineSpacingExtra="5dp"
        app:layout_constraintHeight_percent="0.5"
        app:layout_constraintTop_toBottomOf="@+id/purchase_remind_top_panel"/>
    <LinearLayout
        android:orientation="vertical"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="56dp"
        app:insetType="bottomMargin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent">
        <com.security.xvpn.z35kb.widget.XButton
            android:textSize="@dimen/dialog_purchase_remind_btn_txt_size"
            android:textColor="@color/white"
            android:id="@+id/sign_up_btn"
            android:background="@drawable/bg_purchase_remind_btn"
            android:focusable="true"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dialog_purchase_remind_btn_height"
            android:layout_margin="10dp"
            android:text="@string/SignUp"
            android:textAllCaps="false"
            android:elevation="2dp"/>
        <TextView
            android:textSize="@dimen/dialog_purchase_remind_btn_txt_size"
            android:textColor="@color/purchase_remind_tv_text"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:id="@+id/sign_in_btn"
            android:background="@drawable/bg_purchase_remind_btn_cancel"
            android:padding="4dp"
            android:focusable="true"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dialog_purchase_remind_btn_height"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:text="@string/SignIn"
            android:textAllCaps="false"/>
        <com.security.xvpn.z35kb.widget.XButton
            android:textSize="@dimen/dialog_purchase_remind_btn_txt_size"
            android:textColor="@color/white"
            android:id="@+id/ok_btn"
            android:background="@drawable/bg_purchase_remind_btn"
            android:focusable="true"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dialog_purchase_remind_btn_height"
            android:layout_margin="10dp"
            android:text="@string/Okay"
            android:textAllCaps="false"
            android:elevation="2dp"/>
    </LinearLayout>
</com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
