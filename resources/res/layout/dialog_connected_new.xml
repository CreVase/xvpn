<?xml version="1.0" encoding="utf-8"?>
<com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:tag="layout/dialog_connected_new_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_flag_top"
        android:layout_width="0dp"
        android:layout_height="175dp"
        android:src="@drawable/img_connected_new_top_bg"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/btnClose"
        android:focusable="true"
        android:visibility="visible"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@drawable/icon_nav_cancel_light"
        android:contentDescription="Close"
        app:insetType="topMargin"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/TopBarIconStyle"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="24sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:id="@+id/tv_flag_connect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="@string/Connected"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="18sp"
        android:textColor="@color/white"
        android:id="@+id/tv_flag_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/YouAreSecurelyConnected"
        android:includeFontPadding="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_flag_connect"/>
    <TextView
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#ffc82c"
        android:tag="binding_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:includeFontPadding="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_flag_desc"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/llGuidePurchasePanel"
        android:tag="binding_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        app:insetType="bottomMargin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_flag_top"
        app:layout_constraintWidth_max="420dp">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <LinearLayout
                android:orientation="vertical"
                android:fitsSystemWindows="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.security.xvpn.z35kb.view.GuidePurchaseHeaderView
                    android:id="@+id/ivHeaderView"
                    android:tag="binding_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="vertical"
        android:tag="binding_4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_flag_top">
        <View
            android:tag="binding_5"
            android:background="@drawable/img_connected_dlg_share_shadow"
            android:layout_width="match_parent"
            android:layout_height="16dp"
            app:layout_constraintTop_toBottomOf="@+id/cl_rating"/>
        <View
            android:background="@drawable/img_connected_dlg_share_shadow"
            android:layout_width="match_parent"
            android:layout_height="16dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_share"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_rating"
            android:tag="binding_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:minHeight="50dp"
            app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:id="@+id/iv_rating_like"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_connected_dlg_like"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="12sp"
                android:textColor="#666"
                android:id="@+id/tv_rating_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/ConnectedDlgRateIt"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintBottom_toTopOf="@+id/iv_rating_star"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/iv_rating_like"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/iv_rating_star"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:src="@drawable/img_connected_dlg_stars"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_rating_text"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="vertical"
            android:id="@+id/ll_share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/cl_rating"
            app:layout_goneMarginTop="0dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:src="@drawable/img_connected_dlg_share_cloud"
                android:adjustViewBounds="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:textSize="12sp"
                android:textColor="#666"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:id="@+id/tv_share_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/ConnectedDlgShareTitle"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <Button
                android:textSize="16sp"
                android:textColor="@color/white"
                android:layout_gravity="center_horizontal"
                android:id="@+id/btn_share_with_friends"
                android:background="@drawable/btn_blue_round_full_connected_dlg"
                android:layout_width="188dp"
                android:layout_height="36dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="24dp"
                android:text="@string/ShareWithFriends"
                android:textAllCaps="false"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_share_title"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <FrameLayout
            android:id="@+id/fl_ad_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:minHeight="50dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_share"
            app:layout_goneMarginTop="0dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
