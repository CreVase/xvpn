<?xml version="1.0" encoding="utf-8"?>
<com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/dialog_guide_to_subscribe_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout
        android:id="@+id/content"
        android:tag="binding_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <LinearLayout
            android:layout_gravity="bottom"
            android:orientation="vertical"
            android:id="@+id/ll_bottom_panel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:insetType="bottomMargin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <com.security.xvpn.z35kb.XTextViewNew
                android:textColor="@color/black_lyw"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:id="@+id/tv_flag_1"
                android:tag="binding_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintBottom_toTopOf="@+id/tvPrice"/>
            <com.security.xvpn.z35kb.XTextViewNew
                android:textSize="11sp"
                android:textColor="#666666"
                android:layout_gravity="center_horizontal"
                android:id="@+id/tvPrice"
                android:tag="binding_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"/>
            <Button
                android:textSize="15sp"
                android:id="@+id/btn_subscribe"
                android:tag="binding_4"
                android:focusable="true"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="16dp"
                android:text="@string/Continue"
                android:shadowDy="8"
                android:shadowRadius="10"
                android:textAllCaps="false"
                android:fontFamily="sans-serif-medium"/>
        </LinearLayout>
        <ScrollView
            android:id="@+id/sv"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/ll_bottom_panel"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.security.xvpn.z35kb.view.GuidePurchaseHeaderView
                    android:id="@+id/iv_header"
                    android:tag="binding_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/tv_flag_2"
                    android:paddingBottom="10dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/iv_header">
                    <com.security.xvpn.z35kb.XTextViewNew
                        android:textColor="#666666"
                        android:id="@+id/tvPreTitleTips"
                        android:tag="binding_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="32dp"
                        android:lineSpacingExtra="10dp"/>
                    <com.security.xvpn.z35kb.XTextViewNew
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="#333333"
                        android:id="@+id/tvTopTitle"
                        android:tag="binding_7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:fontFamily="sans-serif-medium"/>
                    <com.security.xvpn.z35kb.XTextViewNew
                        android:textColor="#666666"
                        android:id="@+id/tvSubscribeFeature"
                        android:tag="binding_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:lineSpacingExtra="10dp"
                        android:fontFamily="sans-serif-medium"
                        style="@style/GuideSubscribeTextStyle"/>
                    <Space
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>
                </LinearLayout>
                <TextView
                    android:textSize="12sp"
                    android:textColor="#2aa2ee"
                    android:id="@+id/btn_sign_in"
                    android:background="@drawable/bg_pressed_rectangle_a0"
                    android:padding="10dp"
                    android:focusable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/SignIn"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="@+id/dot"
                    app:layout_constraintTop_toTopOf="@+id/btn_restore"/>
                <View
                    android:id="@+id/dot"
                    android:background="@drawable/dot_middle_purchase"
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    app:layout_constraintBottom_toBottomOf="@+id/btn_sign_in"
                    app:layout_constraintLeft_toRightOf="@+id/btn_sign_in"
                    app:layout_constraintRight_toLeftOf="@+id/btn_restore"
                    app:layout_constraintTop_toTopOf="@+id/btn_sign_in"/>
                <TextView
                    android:textSize="12sp"
                    android:textColor="#2aa2ee"
                    android:id="@+id/btn_restore"
                    android:background="@drawable/bg_pressed_rectangle_a0"
                    android:padding="10dp"
                    android:focusable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/RestorePurchase"
                    app:layout_constraintLeft_toRightOf="@+id/dot"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_flag_2"/>
                <com.security.xvpn.z35kb.XTextViewNew
                    android:textSize="12sp"
                    android:textColor="#999999"
                    android:textColorLink="@color/colorAccent"
                    android:id="@+id/tvPurchaseRule"
                    android:tag="binding_9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="40dp"
                    android:lineSpacingExtra="2.5dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_restore"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/btnClose"
        android:tag="binding_10"
        android:focusable="true"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:insetType="topMargin"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/TopBarIconStyle"/>
</com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
