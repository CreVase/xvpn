<?xml version="1.0" encoding="utf-8"?>
<com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:tag="layout/activity_choose_clear_type_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.security.xvpn.z35kb.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textSize="17sp"
        android:textStyle="bold"
        android:textColor="@color/black_lyw"
        android:id="@+id/tv_flag_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="16dp"
        android:text="@string/CleanBrowser"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"/>
    <include
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_flag_1"
        layout="@layout/divider"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/flag_1"
        android:tag="binding_1"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_flag_1">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textColor="#49475e"
            android:id="@+id/tvTypeLeaveBrowser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/LeaveBrowser"/>
        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <TextView
            android:textSize="13sp"
            android:textColor="#ffc80a"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Premium"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:tag="binding_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_select_search_engine"/>
    </LinearLayout>
    <include
        android:id="@+id/divider_1"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        app:layout_constraintTop_toBottomOf="@+id/flag_1"
        layout="@layout/divider"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:tag="binding_3"
        android:background="@drawable/bg_browser_white_ripple"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        app:layout_constraintTop_toBottomOf="@+id/flag_1">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textColor="#49475e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Click home button/Lock screen"/>
        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <TextView
            android:textSize="13sp"
            android:textColor="#ffc80a"
            android:tag="binding_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Premium"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:tag="binding_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_select_search_engine"/>
    </LinearLayout>
    <include
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/flag_1"
        layout="@layout/divider"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/flag_2"
        android:tag="binding_6"
        android:background="@drawable/bg_browser_white_ripple"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/flag_1">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textColor="#49475e"
            android:id="@+id/tvTypeDontClean"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DoNotCleanAnyway"/>
        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:tag="binding_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_select_search_engine"/>
    </LinearLayout>
</com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
