<?xml version="1.0" encoding="utf-8"?>
<com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/activity_browser_setting_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.security.xvpn.z35kb.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="17sp"
        android:textStyle="bold"
        android:textColor="#333333"
        android:gravity="center_vertical"
        android:id="@+id/flag_search_engine"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginLeft="16dp"
        android:text="@string/SearchEngine"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/ll_search_engine"
        android:background="@drawable/bg_browser_white_ripple"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:clickable="true"
        android:layout_width="0dp"
        android:layout_height="44dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flag_search_engine">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_search_engine"
            android:tag="binding_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:transitionName="share_element"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textStyle="normal"
            android:textColor="#49475e"
            android:gravity="center"
            android:id="@+id/tv_search_engine_name"
            android:tag="binding_2"
            android:paddingLeft="6dp"
            android:paddingRight="6dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <Space
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_arrow_right_gray"/>
    </LinearLayout>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="17sp"
        android:textStyle="bold"
        android:textColor="#333333"
        android:gravity="bottom"
        android:id="@+id/flag_private"
        android:paddingBottom="10dp"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginLeft="16dp"
        android:text="@string/Privacy"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ll_search_engine"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="15sp"
        android:textStyle="normal"
        android:textColor="#49475e"
        android:gravity="center_vertical"
        android:id="@+id/tv_clean_history"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:clickable="true"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:text="@string/CleanBrowsingHistory"
        android:drawableEnd="@drawable/icon_arrow_right_gray"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flag_private"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/flag_block_ads_container"
        android:background="@color/colorWhite"
        android:paddingLeft="16dp"
        android:paddingTop="16dp"
        android:paddingRight="20dp"
        android:paddingBottom="16dp"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_clean_history">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textColor="#49475e"
            android:id="@+id/tv_block_ads"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Block Ads"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="12sp"
            android:textColor="#49475e"
            android:id="@+id/tv_block_ads_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="Block annoying ads on all websites"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_block_ads"/>
        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/btn_switch_block_ads"
            android:background="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:thumb="@drawable/switch_thumb_style"
            app:buttonTint="@color/white"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:showText="false"
            app:track="@drawable/switch_track_style"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="13sp"
            android:textColor="#ffc80a"
            android:tag="binding_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginRight="7.5dp"
            android:text="@string/Premium"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_switch_block_ads"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/flag_block_tracker_container"
        android:background="@color/colorWhite"
        android:paddingLeft="16dp"
        android:paddingTop="16dp"
        android:paddingRight="20dp"
        android:paddingBottom="16dp"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flag_block_ads_container">
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="15sp"
            android:textColor="#49475e"
            android:id="@+id/tv_block_ads_tracker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Block Analytics Trackers"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/btn_switch_block_tracker"
            android:background="@color/white"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:checked="false"
            android:thumb="@drawable/switch_thumb_style"
            app:buttonTint="@color/white"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:showText="false"
            app:track="@drawable/switch_track_style"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="12sp"
            android:textColor="#49475e"
            android:id="@+id/tv_block_ads_tracker_desc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_marginRight="20dp"
            android:text="Stop to collect,analyze and measure activities like tapping and scrolling"
            android:maxLines="2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/btn_switch_block_tracker"
            app:layout_constraintTop_toBottomOf="@+id/tv_block_ads_tracker"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textSize="13sp"
            android:textColor="#ffc80a"
            android:tag="binding_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginRight="7.5dp"
            android:text="Premium"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_switch_block_tracker"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.security.xvpn.z35kb.widget.NavigationBarContentConstraintLayout>
