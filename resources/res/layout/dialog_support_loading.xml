<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_gravity="center"
        android:id="@+id/llContainer"
        android:background="@drawable/privacy_dialog_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <com.security.xvpn.z35kb.widget.LoadingView
            android:id="@+id/ivLoadingView"
            android:layout_width="53dp"
            android:layout_height="53dp"
            android:layout_marginTop="18dp"
            android:layout_centerHorizontal="true"/>
        <com.security.xvpn.z35kb.XTextViewNew
            android:textColor="#999999"
            android:layout_gravity="center"
            android:id="@+id/tvContent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="30dp"
            android:maxWidth="410dp"
            android:layout_below="@+id/ivLoadingView"/>
    </RelativeLayout>
</FrameLayout>
