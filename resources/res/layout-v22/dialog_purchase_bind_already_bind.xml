<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.security.xvpn.z35kb.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="44dp"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="18sp"
        android:textColor="@color/text_normal"
        android:id="@+id/title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="@dimen/dialog_purchase_bind_error_title_margin_top"
        android:layout_marginRight="35dp"
        android:lineSpacingExtra="5dp"
        android:layout_marginHorizontal="35dp"/>
    <com.security.xvpn.z35kb.XTextViewNew
        android:textSize="18sp"
        android:textColor="#465c66"
        android:id="@+id/purchase_remind_notice_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="35dp"
        android:text="@string/AccountAlreadyPremiumDesc"
        android:lineSpacingExtra="5dp"
        android:layout_marginHorizontal="35dp"/>
    <Space
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"/>
    <com.security.xvpn.z35kb.widget.XButton
        android:textSize="@dimen/dialog_purchase_remind_btn_txt_size"
        android:textColor="@color/black_lyw"
        android:layout_gravity="right"
        android:id="@+id/ok_btn"
        android:focusable="true"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_marginBottom="106dp"
        android:text="@string/Okay"
        android:textAllCaps="false"
        android:elevation="2dp"
        android:layout_marginHorizontal="35dp"/>
</LinearLayout>
